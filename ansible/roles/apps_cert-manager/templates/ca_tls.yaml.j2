apiVersion: v1
kind: Secret
metadata:
  name: vm-ca-tls-secret
  annotations:
    kubed.appscode.com/sync: "domain={{ root_domain }}"
    kubed.appscode.com/sync-contexts: baku
  labels:
    app.kubernetes.io/part-of: argocd
type: kubernetes.io/tls
data:
  tls.crt: "{{ lookup('file', ca.cert) | b64encode }}"
  tls.key: "{{ lookup('file', ca.key) | b64encode }}"
